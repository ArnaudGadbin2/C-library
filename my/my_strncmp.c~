/*
** EPITECH PROJECT, 2019
** my_strncmp
** File description:
** task07
*/
int my_strncmp (char const *s, char const *s2, int n)
{
    int c = 0;
    int i = 0;
    int st = s[0];
    int st2 = s2[0];
    while(i < n){
        st = st + s[i];
        i++;
    }
    while(c < n){
        st2 = st2 + s2[c];
        c++;
    }
    return (st - st2);
}
