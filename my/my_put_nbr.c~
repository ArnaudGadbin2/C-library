void my_putchar(char c);
int my_put_nbr(int nb)
{
    if(nb<0)
    {
        nb = -nb;
        my_putchar('-');
    }
    int n = 1;
    int k = 0;
    
    while(nb>n)
    {
        n = n * 10;
        k = k + 1;
    }
    char digit1;
    digit1 = nb / n;
}
